{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "Webpack + Babel + Eslint + Prettier + Sass + React setup",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "webpack": "cross-env NODE_ENV=production webpack",
    "build": "npm-run-all clean webpack",
    "start": "webpack-dev-server --hot --open",
    "eslint": "eslint src/**/*.js",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier src/**/*.js",
    "prettier:write": "npm run prettier -- --write",
    "format": "npm-run-all prettier:write eslint:fix"
  },
  "keywords": [
    "webpack",
    "babel",
    "eslint",
    "prettier",
    "react",
    "setup"
  ],
  "author": "diego.zuniga.mendez@gmail.com",
  "license": "MIT",
  "repository": "https://github.com/dzunigamendez/react-starter-kit",
  "engines": {
    "node": ">= 10.x.x <= 11.x.x"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-standard": "4.0.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "prettier": "1.17.0",
    "react-hot-loader": "4.8.4",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.2",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1"
  },
  "dependencies": {
    "express": "4.16.4",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  }
}
